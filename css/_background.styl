@import _color

stripeSize = 20px

body
  background-image linear-gradient(
  	-45deg,
    background_A 25%,
  	background_B 25%,
  	background_B 50%,
  	background_A 50%,
  	background_A 75%,
  	background_B 75%,
  	background_B
  )
  background-size stripeSize stripeSize
  background-attachment fixed
